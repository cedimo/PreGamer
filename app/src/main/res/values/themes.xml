        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black_our</item>
        <item name="colorPrimaryVariant">@color/purple_our</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/blue_our</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/black_our</item>
        <!-- Customize your theme here. -->
        <item name="android:background">@color/black_our</item>
    </style>
</resources>